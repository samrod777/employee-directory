function Container(props) {\n  return (\n    <div className={`container${props.fluid ? \"-fluid\" : \"\"}`}>\n      {props.children}\n    </div>\n  );\n}\n\nexport default Container;\n","import React from \"react\";\n\nconst styles = {\n  searchField: {\n    display: \"flex\",\n    alignItems: \"center\",\n    justifyContent: \"left\",\n    backgroundColor: \"gray\",\n    padding: \"5px 0 5px 25px\",\n  },\n};\n\nfunction Search(props) {\n  return (\n    <form className=\"form-inline\" style={styles.searchField}>\n      <div className=\"form-group row\">\n        <input\n          onChange={props.handleImputEvent}\n          value={props.value}\n          name=\"search\"\n          type=\"text\"\n          className=\"form-control\"\n          placeholder=\"Search\"\n          id=\"search\"\n        />\n      </div>\n    </form>\n  );\n}\n\nexport default Search;\n","import React from \"react\";\nimport \"./Table.css\";\n\nfunction Table(props) {\n    return (\n        <table className=\"table table-striped table-bright table-hover\">\n        <thead>\n        <tr>\n            <th scope=\"col\">Picture</th>\n            <th value=\"name.first\" scope=\"col\">First Name</th>\n            <th value=\"last\" scope=\"col\" className=\"sort-option\" onClick={props.sortBy}>Last Name</th>\n            <th value =\"phone\" scope=\"col\">Phone</th>\n            <th value=\"ID\" scope=\"col\">ID</th>\n            </tr>\n        </thead>\n        <tbody>\n            {props.children}\n        </tbody>\n        </table>\n    );\n}\n\nexport default Table;","import React from \"react\";\n\nfunction Employee(props) {\n  return (\n    <tr>\n      <th scope=\"row\">\n        <img\n          alt={props.name + props.last + \"picture\"}\n          className=\"img-fluid\"\n          src={props.image}\n        />\n      </th>\n      <td className=\"align-middle\">{props.name}</td>\n      <td className=\"align-middle\">{props.last}</td>\n      <td className=\"align-middle\">{props.phone}</td>\n      <td className=\"align-middle\">{props.ID}</td>\n    </tr>\n  );\n}\n\nexport default Employee;\n","import React from \"react\";\nimport API from \"../utils/API\";\nimport Col from \"./Col\";\nimport Row from \"./Row\";\nimport Container from \"./Container\";\nimport Search from \"./Search\";\nimport Table from \"./Table\";\nimport Employee from \"./Employees\";\n\nclass DirContainer extends React.Component {\n  state = {\n    search: \"\",\n    result: [],\n    sort: \"\",\n    sortedList: [],\n  };\n\n  componentDidMount() {\n    this.searchEmployees(\"?results=500\");\n  }\n\n  searchEmployees = (query) => {\n    API.search(query)\n      .then((res) =>\n        this.setState({\n          result: res.data.results,\n          sortedList: res.data.results,\n        })\n      )\n      .catch((err) => console.log(err));\n  };\n\n  sortBy = () => {\n    if (this.state.sort === \"\" || this.state.sort === \"asc\") {\n      let sorted = this.state.sortedList.sort((a, b) => {\n        if (a.name.last < b.name.last) return -1;\n        else if (a.name.last > b.name.last) return 1;\n        return 0;\n      });\n      this.setState({ sortedList: sorted, sort: \"desc\" });\n    } else {\n      let sorted = this.state.sortedList.sort((a, b) => {\n        if (a.name.last > b.name.last) return -1;\n        else if (a.name.last < b.name.last) return 1;\n        return 0;\n      });\n      this.setState({ sortedList: sorted, sort: \"asc\" });\n    }\n  };\n\n  handleImputEvent = (e) => {\n    const name = e.target.name;\n    const value = e.target.value;\n    const sortedList = this.state.result.filter((item) => {\n      let names = Object.values(item).join(\"\");\n      return names.indexOf(value) !== -1;\n    });\n    this.setState({\n      [name]: value,\n      sortedList: sortedList,\n    });\n  };\n\n  render() {\n    return (\n      <Container>\n        <Row>\n          <Col size=\"md-12\">\n            <Search\n              value={this.state.search}\n              handleImputEvent={this.handleImputEvent}\n            />\n          </Col>\n        </Row>\n        <Row>\n          <Col size=\"md-12\">\n            <Table sortBy={this.sortBy}>\n              {this.state.sortedList.map((item) => (\n                <Employee\n                  key={item.login.uuid}\n                  image={item.picture.medium}\n                  name={item.name.first}\n                  last={item.name.last}\n                  phone={item.phone}\n                  ID={item.id.value}\n                />\n              ))}\n            </Table>\n          </Col>\n        </Row>\n      </Container>\n    );\n  }\n}\n\nexport default DirContainer;\n","import React from \"react\";\nimport Jumbotron from \"./components/Jumbotron\";\nimport DirContainer from \"./components/DirContainer\";\n\nfunction App() {\n  return (\n    <div className=\"App\">\n      <Jumbotron />\n      <DirContainer />\n    </div>\n  );\n}\n\nexport default App;\n","import React from \"react\";\nimport ReactDOM from \"react-dom\";\nimport App from \"./App\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\n\nReactDOM.render(<App />, document.getElementById(\"root\"));\n"]}}